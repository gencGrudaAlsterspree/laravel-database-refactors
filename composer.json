{
    "name": "signifly/laravel-database-refactors",
    "description": "Add database refactors to your Laravel app.",
    "homepage": "https://github.com/signifly/laravel-database-refactors",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\DatabaseRefactors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\DatabaseRefactors\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\DatabaseRefactors\\RefactorServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
